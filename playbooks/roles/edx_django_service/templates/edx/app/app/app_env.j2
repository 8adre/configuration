# {{ ansible_managed }}
{% for name,value in edx_django_service_environment.items() -%}
{%- if ( (edx_django_service_is_devstack is sameas true) and
        ('_CFG' in name))
-%}
export REGISTRAR_CFG="{{ COMMON_APP_DIR }}/{{ edx_django_service_name }}/{{ edx_django_service_name }}/{{ edx_django_service_name }}/settings/{{ edx_django_service_name }}.yml"
{%- else -%}
export {{ name }}="{{ value }}"
{% endif %}
{%- endfor %}

