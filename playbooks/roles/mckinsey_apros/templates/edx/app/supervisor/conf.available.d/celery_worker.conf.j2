[program:celery_worker_default]

command={{ mcka_apros_app_dir }}/worker_default.sh

user={{ common_web_user }}
directory={{ mcka_apros_code_dir }}

stdout_logfile={{ supervisor_log_dir }}/%(program_name)-stdout.log
stderr_logfile={{ supervisor_log_dir }}/%(program_name)-stderr.log
killasgroup=true
stopasgroup=true
autorestart=true

[program:celery_worker_high]

command={{ mcka_apros_app_dir }}/worker_high.sh

user={{ common_web_user }}
directory={{ mcka_apros_code_dir }}

stdout_logfile={{ supervisor_log_dir }}/%(program_name)-stdout.log
stderr_logfile={{ supervisor_log_dir }}/%(program_name)-stderr.log
killasgroup=true
stopasgroup=true
autorestart=true
